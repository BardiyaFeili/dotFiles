{
  "project_panel": {
    "dock": "right"
  },
  "vim_mode": true,
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "ui_font_size": 17,
  "scrollbar": {
    "show": "never"
  },
  "buffer_font_size": 17,
  "theme": "Tokyo Night",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "terminal": {
    "dock": "bottom",
    "font_family": "JetBrainsMono Nerd Font"
  },
}
